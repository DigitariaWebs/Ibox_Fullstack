{
  "expo": {
    "name": "iBox",
    "slug": "ibox",
    "scheme": "ibox",
    "linking": {
      "schemes": ["ibox"]
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.yourcompany.ibox",
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses location to provide better delivery services",
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSAllowsLocalNetworking": true,
          "NSExceptionDomains": {
            "192.168.1.12": {
              "NSTemporaryExceptionAllowsInsecureHTTPLoads": true
            },
            "172.25.8.223": {
              "NSTemporaryExceptionAllowsInsecureHTTPLoads": true
            },
            "localhost": {
              "NSTemporaryExceptionAllowsInsecureHTTPLoads": true
            }
          }
        }
      }
    },
    "android": {
      "package": "com.yourcompany.ibox",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAzPxqQ9QhUq_cmXkkcE-6DcgJn-EDngzI"
        }
      }
    },
    "web": {
      "favicon": "./assets/images/logo-icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-video"
    ],
    "platforms": [
      "ios",
      "android"
    ]
  }
}
